{{#each this}}
<li class = "country-card">
    <div class="country__head">
     <img class="country__img" src="{{flags.svg}}" alt="Flag" width="50" height="25">
     <h1 class="country__title, head-title">{{name.official}}</h1>
     </div>
     <div class="country__info">
        <div class="info">
           <h2>Capital:</h2>
           <p>{{capital}}</p>
        </div>
        <div class="info">
           <h2> Population:</h2>
           <p>{{population}}</p>
        </div>
        <div class="info">
           <h2>Languages:</h2>
           {{#each languages}}
           <p>{{this}}</p>
        </div>
    {{/each}}
    </div>
</li>
{{/each}}